{
  "expo": {
    "name": "Uber",
    "slug": "uber",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "android": {
            "backgroundColor": "#0c7cff",
            "image": "./assets/images/splash.png",
            "imageWidth": 150
          },
          "ios": {
            "backgroundColor": "#ffffff",
            "image": "./assets/images/splash.png",
            "resizeMode": "cover"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
